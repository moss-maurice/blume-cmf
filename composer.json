{
    "name": "mmaurice/laura-project",
    "type": "project",
    "description": "Laura (&) is Laravel based CMF project",
    "keywords": [
        "laura",
        "cms",
        "cmf",
        "laravel",
        "framework"
    ],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Moss Maurice",
            "email": "kreexus@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "ext-pdo": "*",
        "ext-json": "*",
        "bit3/git-php": "^1.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Blume\\": "system/core/",
            "Database\\Factories\\": "system/database/factories/",
            "Database\\Seeders\\": "system/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "system/tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php system/artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php system/artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('system/.env') || copy('system/.env.example', 'system/.env');\""
        ],
        "post-create-project-cmd": [
            "@php system/artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "vendor-dir": "system/vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
